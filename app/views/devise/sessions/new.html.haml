%section.container
	.row
		=form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
			%legend Sign in
			%fieldset
				= f.label :email
				= f.email_field :email

				= f.label :password
				= f.password_field :password

			-if devise_mapping.rememberable?
				= f.check_box :remember_me 
				= f.label :remember_me
			.form-actions
				=f.submit "Sign in"
			%fieldset
				= render "devise/shared/links"

		
